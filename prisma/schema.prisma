
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/sample_db?schema=public"
}
model Player {
  @@map("player")

  id      Int @id() @default(autoincrement())
  name    String
  level   Int @default(1)
  country String

}
